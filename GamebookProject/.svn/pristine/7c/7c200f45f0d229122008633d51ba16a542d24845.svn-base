package pl.polsl.ptakjakub.gamebook.dto;

/**
 * Represents 'range' node.
 *
 * @author Jakub Ptak
 * @version 1.0
 */
public class Range {

    private int from;

    private int to;

    private Integer nextParagraphId;

    /**
     * Gets from bound of a range.
     *
     * @return from bound.
     */
    public int getFrom() {
        return from;
    }

    /**
     * Sets from bound of a range.
     *
     * @param from bound
     */
    public void setFrom(int from) {
        this.from = from;
    }

    /**
     * Gets to bound of a range.
     *
     * @return to bound
     */
    public int getTo() {
        return to;
    }

    /**
     * Sets to bound of a range.
     *
     * @param to bound
     */
    public void setTo(int to) {
        this.to = to;
    }

    /**
     * Gets next paragraph id.
     *
     * @return id
     */
    public Integer getNextParagraphId() {
        return nextParagraphId;
    }

    /**
     * Sets next paragraph id.
     *
     * @param nextParagraphId paragraph id
     */
    public void setNextParagraphId(Integer nextParagraphId) {
        this.nextParagraphId = nextParagraphId;
    }
}
