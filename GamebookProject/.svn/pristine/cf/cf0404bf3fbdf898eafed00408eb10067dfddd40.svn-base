package pl.polsl.ptakjakub.gamebook.dto;

/**
 * Class representing an item.
 *
 * @author Jakub Ptak
 * @version 1.0
 */
public class Item {

    private int id;

    private String name;

    private String type;

    private String attribute;

    private Integer value;

    /**
     * Gets type of an item.
     *
     * @return type
     */
    public String getType() {
        return type;
    }

    /**
     * Sets type of an item.
     *
     * @param type name
     */
    public void setType(String type) {
        this.type = type;
    }

    /**
     * Gets attribute of an item.
     *
     * @return attribute
     */
    public String getAttribute() {
        return attribute;
    }

    /**
     * Sets attribute of an item.
     *
     * @param attribute name
     */
    public void setAttribute(String attribute) {
        this.attribute = attribute;
    }

    /**
     * Gets value of item influence.
     *
     * @return value
     */
    public Integer getValue() {
        return value;
    }

    /**
     * Sets value of item influence.
     *
     * @param value number
     */
    public void setValue(Integer value) {
        this.value = value;
    }

    /**
     * Gets id of an item.
     *
     * @return id
     */
    public int getId() {
        return id;
    }

    /**
     * Sets id of and item.
     *
     * @param id number
     */
    public void setId(int id) {
        this.id = id;
    }

    /**
     * Gets name of an item.
     *
     * @return name
     */
    public String getName() {
        return name;
    }

    /**
     * Sets name of an item.
     *
     * @param name String
     */
    public void setName(String name) {
        this.name = name;
    }
}
