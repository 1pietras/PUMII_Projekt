package pl.polsl.ptakjakub.gamebook.exceptions;

/**
 * Exception class of gamebook application.
 *
 * @author Jakub Ptak
 * @version 1.0
 */
public class GamebookException extends Exception {

    /**
     * Constructor initialized an exception.
     */
    public GamebookException() { super(); }

    /**
     * Constructor initialized an exception.
     * @param message String
     */
    public GamebookException(String message) { super(message); }

    /**
     * Constructor initialized an exception.
     *
     * @param message String
     * @param cause Throwable
     */
    public GamebookException(String message, Throwable cause) { super(message, cause); }

    /**
     * Constructor initialized an exception.
     *
     * @param cause Throwable
     */
    public GamebookException(Throwable cause) { super(cause); }
}
