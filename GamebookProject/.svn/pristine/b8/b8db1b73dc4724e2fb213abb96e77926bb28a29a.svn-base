package pl.polsl.ptakjakub.gamebook.dto;

/**
 * Represents effect node.
 *
 * @author Jakub Ptak
 * @version 1.0
 */
public class Effect {

    private String attribute;

    private Integer value;

    /**
     * Gets attribute of the effect.
     *
     * @return attribute
     */
    public String getAttribute() {
        return attribute;
    }

    /**
     * Sets attribute of the effect.
     *
     * @param attribute name
     */
    public void setAttribute(String attribute) {
        this.attribute = attribute;
    }

    /**
     * Gets value of the effect.
     *
     * @return value
     */
    public Integer getValue() {
        return value;
    }

    /**
     * Sets value of the effect.
     *
     * @param value number
     */
    public void setValue(Integer value) {
        this.value = value;
    }
}

