package pl.polsl.ptakjakub.gamebook.paragraphs;

import pl.polsl.ptakjakub.gamebook.dto.Path;

/**
 * Class representing 'checkluck' type paragraph.
 *
 * @author Jakub Ptak
 * @version 1.0
 */
public class CheckluckParagraph extends Paragraph {

    private Path luckyPath;

    private Path unluckPath;

    /**
     * Gets path in case of having luck
     *
     * @return path
     */
    public Path getLuckyPath() {
        return luckyPath;
    }

    /**
     * Sets path in case of having luck
     *
     * @param luckyPath Path for having luck
     */
    public void setLuckyPath(Path luckyPath) {
        this.luckyPath = luckyPath;
    }

    /**
     * Gets path in case of not having luck
     *
     * @return path
     */
    public Path getUnluckPath() {
        return unluckPath;
    }

    /**
     * Sets path in case of not having luck
     *
     * @param unluckPath Path for not having luck
     */
    public void setUnluckPath(Path unluckPath) {
        this.unluckPath = unluckPath;
    }
}
